//without using third variable
#include<iostream>
using namespace std;

int main()
{
    int x,y;
    cout << "Enter two numbers:-";
    cin >> x>>y;
    cout << "Numbers before swapping are:-"<<x<<"\t"<<y;
    x=x+y;
    y=x-y;
    x=x-y;
    cout << "\nNumbers after swapping are:-"<<x<<"\t"<<y;
    
    return 0;
}
